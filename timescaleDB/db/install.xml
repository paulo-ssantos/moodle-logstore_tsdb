<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="logstore/timescaledb/db" VERSION="2023112500" COMMENT="DB schema para o plugin Logstore TimescaleDB">
    <TABLES>
        <TABLE NAME="moodle_events" COMMENT="Tabela para logs do Moodle no TimescaleDB">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="time" TYPE="datetime" NOTNULL="true"/>
                <FIELD NAME="eventname" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="component" TYPE="char" LENGTH="100"/>
                <FIELD NAME="action" TYPE="char" LENGTH="100"/>
                <FIELD NAME="target" TYPE="char" LENGTH="100"/>
                <FIELD NAME="crud" TYPE="char" LENGTH="1"/>
                <FIELD NAME="edulevel" TYPE="int" LENGTH="4"/>
                <FIELD NAME="anonymous" TYPE="int" LENGTH="4" DEFAULT="0"/>
                <FIELD NAME="courseid" TYPE="int" LENGTH="10"/>
                <FIELD NAME="contextid" TYPE="int" LENGTH="10"/>
                <FIELD NAME="contextlevel" TYPE="int" LENGTH="4"/>
                <FIELD NAME="contextinstanceid" TYPE="int" LENGTH="10"/>
                <FIELD NAME="userid" TYPE="int" LENGTH="10"/>
                <FIELD NAME="relateduserid" TYPE="int" LENGTH="10"/>
                <FIELD NAME="realuserid" TYPE="int" LENGTH="10"/>
                <FIELD NAME="objectid" TYPE="int" LENGTH="10"/>
                <FIELD NAME="objecttable" TYPE="char" LENGTH="255"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10"/>
                <FIELD NAME="ip" TYPE="char" LENGTH="45"/>
                <FIELD NAME="origin" TYPE="char" LENGTH="20"/>
                <FIELD NAME="other" TYPE="text"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
